    "homepage": "https://github.com/webextensions/revisitor#readme",
    "license": "MIT",
    "main": "server/application.ts",
    "name": "revisitor",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/webextensions/revisitor.git"
    },
    "scripts": {
        "all-is-well": "./scripts/health-checks/all-is-well.sh",
        "build": "npm run build:development:local",
        "build:development:local": "webpack --disable-interpret --mode development --watch --progress --env config=\"./config/config.development.local.js\"",
        "build:development:local:inspect-brk": "node --inspect-brk ./node_modules/.bin/webpack-cli --disable-interpret --mode development         --progress --env config=\"./config/config.development.local.js\"",
        "build:inspect-brk": "npm run build:development:local:inspect-brk",
        "build:production:local": "NODE_ENV=production webpack --disable-interpret --mode production  --watch --progress --env config=\"./config/config.production.local.js\"",
        "build:production:local:inspect-brk": "NODE_ENV=production node --inspect-brk ./node_modules/.bin/webpack-cli --disable-interpret --mode production          --progress --env config=\"./config/config.production.local.js\"",
        "housekeeping:clean": "./scripts/housekeeping/clean.ts",
        "housekeeping:review-dependencies": "./scripts/housekeeping/imperfect/review-dependencies-via-depcheck.sh",
        "housekeeping:update-package-cjson-and-json": "./scripts/housekeeping/update-package-cjson-and-json.sh",
        "housekeeping:update-package-lock-json": "./scripts/housekeeping/update-package-lock-json.sh",
        "lint": "eslint . --ext .cjs,.js,.mjs,.ts,.tsx --report-unused-disable-directives",
        "lint:fix": "eslint . --ext .cjs,.js,.mjs,.ts,.tsx --report-unused-disable-directives --fix",
        "mocha": "mocha",
        "postversion": "git push --follow-tags",
        "preinstall": "./scripts/npm-run-scripts/preinstall.sh",
        "prepare": "husky || true",
        "server:development:local": "node               server/server.ts --config config/config.development.local.js",
        "server:development:local:http-port-dynamic": "HTTP_PORT_DYNAMIC=yes node      server/server.ts --config config/config.development.local.js",
        "server:development:local:inspect-brk": "node --inspect-brk server/server.ts --config config/config.development.local.js",
        "server:development:local:use-hmr": "USE_HMR=yes node               server/server.ts --config config/config.development.local.js",
        "server:production:local": "NODE_ENV=production node               server/server.ts --config config/config.production.local.js",
        "server:production:local:inspect-brk": "NODE_ENV=production node --inspect-brk server/server.ts --config config/config.production.local.js",
        "start": "npm run start:app",
        "start:app": "concurrently \"npm run start:server\" \"npm run start:build\"",
        "start:app:use-hmr": "npm run start:server:use-hmr",
        "start:build": "npm run build",
        "start:build:inspect-brk": "npm run build:inspect-brk",
        "start:server": "npm run server:development:local",
        "start:server:inspect-brk": "npm run server:development:local:inspect-brk",
        "start:server:use-hmr": "npm run server:development:local:use-hmr",
        "test": "npm run all-is-well",
        "version": "./scripts/build-and-release/prepare-version/prepare-version.sh"
    },
    "type": "module",
    "version": "1.0.0"
}
